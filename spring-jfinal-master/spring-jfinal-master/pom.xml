	<slf4j.version>1.6.2</slf4j.version>
	<logback.version>1.1.2</logback.version>
	<!-- json -->
	<jackson.version>1.9.6</jackson.version>
	<fasterxml.jackson.version>2.1.4</fasterxml.jackson.version>
