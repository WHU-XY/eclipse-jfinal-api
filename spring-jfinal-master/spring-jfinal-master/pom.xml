        <version>2.8.1</version>
    </dependency>
    <dependency>
        <groupId>de.ruedigermoeller</groupId>
        <artifactId>fst</artifactId>
        <version>2.47</version>
    </dependency>
    <!-- 基于Java的任务调度框架 -->
