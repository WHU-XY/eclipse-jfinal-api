    </dependency>
    
    <dependency>
        <groupId>org.jsoup</groupId>
        <artifactId>jsoup</artifactId>
        <version>1.9.2</version>
    </dependency>
    
    <!-- solr基于Lucene的全文搜索服务器 -->
    <!-- https://mvnrepository.com/artifact/org.apache.solr/solr -->
